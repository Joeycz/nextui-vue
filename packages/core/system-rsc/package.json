{
  "name": "@nextui-vue/system-rsc",
  "version": "2.0.11",
  "description": "NextUI system primitives compatibles with RSC imports",
  "keywords": [
    "system-rsc"
  ],
  "author": "Joeycz <muxiesan1989@gmail.com>",
  "homepage": "",
  "license": "MIT",
  "main": "src/index.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Joeycz/nextui-vue.git",
    "directory": "packages/core/system"
  },
  "bugs": {
    "url": "https://github.com/Joeycz/nextui-vue/issues"
  },
  "scripts": {
    "build": "tsup src --dts",
    "build:fast": "tsup src",
    "dev": "pnpm build:fast --watch",
    "clean": "rimraf dist .turbo",
    "typecheck": "tsc --noEmit",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "peerDependencies": {
    "@nextui-vue/theme": ">=2.1.0",
    "tailwind-variants": ">=0.1.13"
  },
  "devDependencies": {
    "@nextui-vue/shared-utils": "workspace:^",
    "@nextui-vue/theme": "workspace:*",
    "clean-package": "2.2.0",
    "react": "^18.2.0",
    "tailwind-variants": "^0.1.18",
    "vue": "^3.4.21"
  },
  "dependencies": {
    "clsx": "^1.2.1"
  },
  "clean-package": "../../../clean-package.config.json",
  "tsup": {
    "clean": true,
    "target": "es2019",
    "format": [
      "cjs",
      "esm"
    ]
  }
}
